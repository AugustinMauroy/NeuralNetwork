{
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "single"],
		"indent": ["error", "tab"],
		"eol-last": ["error", "always"],
		"no-trailing-spaces": ["error", { "skipBlankLines": true }]
	},
	"env": {
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module"
	}
}